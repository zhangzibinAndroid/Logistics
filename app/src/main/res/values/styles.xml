<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="TextViewStyleSelect">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/px25</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/px10</item>
        <item name="android:textColor">@color/textselfirst</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="TextViewStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/px40</item>
        <item name="android:textSize">@dimen/px25</item>
    </style>

    <style name="ViewStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/px02</item>
        <item name="android:background">#E9EDEE</item>
        <item name="android:layout_marginLeft">@dimen/px40</item>
        <item name="android:layout_marginRight">@dimen/px40</item>
    </style>

</resources>
